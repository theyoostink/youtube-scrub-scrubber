{
	"manifest_version": 3,
	"name": "Youtube Scrub Scrubber",
	"description": "This extension blurs thumbnails from the Youtube home page that have low views",
	"version": "1.0.0",
	"host_permissions": ["https://*.youtube.com/*"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"permissions": ["tabs"]
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["https://www.youtube.com/"],
			"run_at": "document_end"
		}
	]
}